    "stylelint-scss": "^3.9.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-url-loader": "^2.3.3",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
