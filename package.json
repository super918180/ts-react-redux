{
  "name": "typescript-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Spencer",
  "license": "MIT",
  "scripts": {
    "analyze": "yarn run build --report",
    "build": "webpack --config tools/webpack.config.ts --mode production --progress --prod",
    "lint:ts": "tslint \"{src, tools}/**/*.{ts,tsx}\"",
    "lint:css": "stylelint \"{src,tools}/**/*.{css,less,scss,sass}\"",
    "lint": "yarn lint:ts && yarn lint:css",
    "start": "webpack-dev-server --config tools/webpack.config.ts --mode development --progress --hot --dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,less,scss,sass}": [
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
